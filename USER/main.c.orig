#include "stm32f10x.h"
#include "timer.h"
#include "delay.h"
#include "usart.h"
#include "key.h"
#include "usart2.h"

//数字舵机，顺时针最左0.6ms 最右2.7ms，PWM频率50HZ
int main(void)
{
    NVIC_PriorityGroupConfig(NVIC_PriorityGroup_2);
	delay_init();
	KEY_Init();
    TIM3_PWM_Init(2000,720);  //72M/720=100KHz/50=2000  PWM频率为50Hz,每2000为20ms
	uart_init(115200);
	USART_2_Init(Bound_usart_2);    
	while(1)
	{
	}
}
